{
  "useTabs": false,
  "printWidth": 100,
  "tabWidth": 2,
  "singleQuote": false,
  "trailingComma": "es5",
  "parser": "typescript",
  "semi": true,
  "jsxSingleQuote": false,
  "bracketSpacing": true,
  "htmlWhitespaceSensitivity": "css",
  "endOfLine": "lf",
  "arrowParens": "always",
  "importOrder": ["<THIRD_PARTY_MODULES>",  "^[./(.*)$]"],
  "overrides": [
    {
        "files": "*.scss",
        "options": {
            "parser": "scss"
        }
    },
    {
        "files": "*.json",
        "options": {
            "parser": "json"
        }
    },
    {
        "files": "*.tsx",
        "options": {
            "parser": "typescript"
        }
    }
    ]
}
